{
  "compilerOptions": {
    "lib": ["es2015"],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": ".build",
    "strict": true,
    "strictPropertyInitialization": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true
  }
}
